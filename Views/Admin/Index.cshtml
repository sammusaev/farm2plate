
@model farm2plate.Models.SOrder;



@{
    ViewData["Title"] = "Admin Dashboard";
}


@{
    ViewBag.Title = "Index";
}

<h1>Manage Reports</h1>

<div class="row">
    <div class="col-xl-8 col-md-6">
        <div class="card bg-primary text-white mb-4">
            <div class="card-body">Order Report</div>
            <div class="card-footer d-flex align-items-center justify-content-between">
                <a class="small text-white stretched-link" asp-action="ViewAllOrder" asp-controller="Admin"></a>
                <div class="small text-white"><i class="fas fa-angle-right"></i></div>
            </div>
        </div>
    </div>
    <div class="col-xl-4 col-md-6">
        <div class="card bg-success text-white mb-4">
            <div class="card-body">All Shops</div>
            <div class="card-footer d-flex align-items-center justify-content-between">
                <a class="small text-white stretched-link" asp-action="ViewAllShop" asp-controller="Admin" ></a>
                <div class="small text-white"><i class="fas fa-angle-right"></i></div>
            </div>
        </div>
    </div>

</div>

